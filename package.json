{
  "name": "react-native-image-builder",
  "version": "1.4.0",
  "description": "Type safe code generation for images targeted at React Native",
  "main": "build/index.js",
  "bin": "build/cli.js",
  "types": "build/index",
  "files": [
    "build"
  ],
  "scripts": {
    "clean-build": "yarn run clean && yarn run build",
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json",
    "test": "tap --no-coverage tests/*.ts",
    "cover": "nyc --reporter=lcov npm test",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gas-buddy/react-native-image-builder.git"
  },
  "keywords": [
    "react-native",
    "svg",
    "images",
    "typescript",
    "gasbuddy"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/glob": "^7.1.3",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.upperfirst": "^4.3.6",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.27",
    "@types/rimraf": "^3.0.0",
    "@types/sharp": "^0.25.1",
    "@types/tap": "^14.10.0",
    "@typescript-eslint/parser": "^3.7.1",
    "cross-env": "^7.0.0",
    "dir-compare": "^2.3.0",
    "eslint": "^7.5.0",
    "rimraf": "^3.0.2",
    "tap": "^14.10.8",
    "typescript": "^3.9.7"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "src"
    ],
    "sourceMap": true,
    "reporter": [
      "lcov"
    ]
  },
  "dependencies": {
    "@svgr/core": "^5.4.0",
    "@svgr/plugin-prettier": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "glob": "^7.1.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "minimist": "^1.2.5",
    "mkdirp": "^1.0.4",
    "p-map": "^4.0.0",
    "sharp": "^0.25.4"
  }
}
